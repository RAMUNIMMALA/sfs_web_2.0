@{
    var LoginTitle = "";
    sfs_model.Partner _Partner = new sfs_model.Partner();
}
@{
    _Partner = (sfs_model.Partner)Session["UserData"];
    LoginTitle = _Partner.FirstName.ToUpper() + " " + _Partner.LastName.Substring(0, 1).ToUpper();
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="thailottoup">
    <meta name="author" content="thailottoup">
    <title>@Page.Title</title>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,300,700' rel='stylesheet' type='text/css'>
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/thai-admin.css" rel="stylesheet">
    <link href="~/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <!--[if lt IE 9]><![endif]-->
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <link rel="stylesheet" href="/resources/demos/style.css">
    <link rel="stylesheet" href="/resources/demos/style.css">

    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />
    <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>

    <style type="text/css">
        body {
            padding: 10px;
            font-family: Arial, Helvetica, sans-serif;
            line-height: 1.5;
            font-size: 14px;
        }

        p {
            margin: 0 0 20px;
            padding: 0;
        }

        a {
            color: #4071b6;
        }

        fieldset {
            border: 1px solid #b4c1e7;
            margin: 0 0 15px;
            padding: 5px 10px;
            width: 98%;
        }

            fieldset legend {
                color: #2e62a3;
                font-weight: bold;
                margin: 0;
                padding: 0 8px;
            }

        .table1 {
            border-collapse: collapse;
        }

            .table1, .table1 th, .table1 td {
                border: 1px solid #d4d4d1;
            }

                .table1 th {
                    font-weight: normal;
                }

                .table1 td {
                    padding: 5px;
                }

        .table-responsive {
            border: 1px solid #ddd;
            margin-bottom: 15px;
            min-height: 0.01%;
            overflow-x: auto;
            overflow-y: hidden;
            width: 100%;
        }
    </style>
    @RenderSection("head", required: false)
</head>
<body>

    <div id="wrapper">
        <!-- Navigation -->
        <nav id="main-navigation" class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse"> <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
                <a class="navbar-brand" href="~/Manager/Index"><img src="~/images/sfs-logo.png" alt="SFS - Logo"></a>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> @LoginTitle <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li> <a href="~/Partner/ResetPassword"><i class="fa fa-fw fa-gear"></i> Manager</a> </li>
                        <li> <a href="~/Home/Index"><i class="fa fa-fw fa-power-off"></i> Log Out</a> </li>
                    </ul>
                </li>
            </ul>
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active"> <a href="/Manager/Index"><i class="fa fa-fw fa-dashboard"></i> Dashboard</a> </li>
                    <li> <a href="/Partner/PartnerList"><i class="fa fa-fw fa-sitemap"></i> Partners </a> </li>
                    <li> <a href="/Plan/PlanList"><i class="fa fa-fw fa-key"></i> Plans</a> </li>
                    <li> <a href="/Payment/PaymentList"><i class="fa fa-fw fa-money"></i> Payment Info</a> </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>
        <div>
         <h3 style="color:greenyellow">@ViewBag.SuccessMessage</h3>    <h3 style="color:orange">@ViewBag.AlertMessage</h3>  
        </div>
        <div id="page-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    <script src="~/js/jquery.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.min.js"></script>
    <!-- Morris Charts JavaScript -->
    <script src="~/js/plugins/morris/raphael.min.js"></script>
    <script src="~/SFS-js/SFS_Validation.js"></script>
    <script src="~/SFS-js/PartnerPlanMapping.js"></script>
    <script src="~/SFS-js/PlanPartnerMapping.js"></script>

</body>
</html>
