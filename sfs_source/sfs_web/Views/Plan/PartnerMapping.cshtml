@model sfs_model.Partner
@{
    ViewBag.Title = "Partner Mapping";
    Layout = "~/Views/Shared/_PartnerLayout.cshtml";
}

<section id="content-section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="title"> </h1>
            </div>
        </div>
        <div class="row">
            <div style="width:80%;">
                <div class="row">
                    <div class="col-xs-11">
                        @using (Html.BeginForm("AddPartnerPlan", "Plan", FormMethod.Post, new { @class = "form-horizontal" }))
                        {
                            <div class="well col-sm-offset-2">
                                <h2 class="title">Member <span class="text-success">Mapping</span></h2>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="password">Plan Title</label>
                                    <div class="col-md-9">
                                        @Html.DropDownListFor(x => x.PlanCode, new SelectList(Model.PMapping, "PlanCode", "Title"), new { @id = "txtPlanCode", @class = "form-control", @required = "", @placeholder = "Select Type" })
                                    </div>
                                </div>

                                <div class="form-group">

                                    <label class="col-md-3 control-label" for="password">Partner Name</label>
                                    <div class="col-md-9">

                                        @Html.DropDownListFor(x => x.PartnerCode, new SelectList(Model.PType, "Id", "Title"), new { @id = "txtPartnerCode", @class = "form-control", @required = "", @placeholder = "Select Type" })
                                    </div>
                                </div>
                                <div class="text-right">
                                    <a href="/Manager/Index"><input class="btn btn-primary btn-lg" id="Cancel" name="btnLogin" value="Cancel" /></a>&nbsp;
                                    <input class="btn btn-primary btn-lg" id="btnsubmit" name="btnLogin" value="Map" type="submit" />
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>




